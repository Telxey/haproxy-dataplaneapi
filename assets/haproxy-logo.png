<style>
    :root{
        --x: 0;
        --y: 0;
    }

    body {
        background: linear-gradient(to bottom, #87CEEB, #ddeeff) no-repeat;
        background-size: cover;
        min-height: 100vh;
        overflow: hidden;
      }
    
      header, footer, .newsbar {
        display: none;
      }

        /** Balloons */
        g#firstBalloon {
            animation: firstBalloons 2s infinite;
            animation-timing-function: ease-in-out;
            animation-direction: alternate;
            animation-delay: 0.5s;
        }

        g#middleBalloon {
            animation: middleBalloons 2s infinite;
            animation-timing-function: ease-in-out;
            animation-direction: alternate;
            animation-delay: 0.4s;
        }

        g#lastBalloon {
            animation: lastBalloons 2s infinite;
            animation-timing-function: ease-in-out;
            animation-direction: alternate;
            animation-delay: 0.6s;
        }

        g#head {
            animation: head 2s infinite;
            animation-timing-function: ease-in-out;
            animation-direction: alternate;
            animation-delay: 0.6s;
        }

        g#tail {
            animation: tail 2s infinite;
            animation-timing-function: ease-in-out;
            animation-direction: alternate;
            animation-delay: 0.6s;
        }

        g#loady {
            animation: loady 2s infinite;
            animation-timing-function: ease-in-out;
            animation-direction: alternate;
            animation-delay: 0.6s;
        }

        @keyframes firstBalloons {
            0% {
                transform-origin: center;
                transform: rotate(0deg) translate3d(0, 0, 0);
            }

            100% {
                transform-origin: center;
                transform: rotate(-5deg) translate3d(0, -3%, 0);
            }
        }

        @keyframes middleBalloons {
            0% {
                transform-origin: center;
                transform: rotate(0deg) translate3d(0, 0, 0);
            }

            100% {
                transform-origin: center;
                transform: rotate(-0.8deg) translate3d(0, -0.8%, 0);
            }
        }

        @keyframes lastBalloons {
            0% {
                transform-origin: center;
                transform: rotate(0deg) translate3d(0, 0, 0);
            }

            100% {
                transform-origin: center;
                transform: rotate(-1deg) translate3d(-1%, 0%, 0);
            }
        }

        @keyframes head {
            0% {
                transform-origin: 55px center;
                transform: rotate(0deg) translate3d(0, 0, 0);
            }

            100% {
                transform-origin: 55px center;
                transform: rotate(-4deg) translate3d(-1%, 0%, 0);
            }
        }

        @keyframes tail {
            0% {
                transform-origin: center;
                transform: rotate(0deg) translate3d(0, 0, 0);
            }

            100% {
                transform-origin: center;
                transform: rotate(1.5deg) translate3d(0%, 0.8%, 0);
            }
        }

        @keyframes loady {
            0% {
                transform-origin: center;
                transform: rotate(0deg) translate3d(0, 0,0);
            }

            100% {
                transform-origin: center;
                transform: rotate(3deg) translate3d(0%, 1%,0);
            }
        }

        /** Clouds */
        .cloud, .cloud img{ 
            height: 200px;
        }

        .cloud{
            animation: fade 1s linear, sway 3s linear infinite alternate;
        }

        @keyframes fade {
            0% {
                opacity: 0;
            }

            100% {
                opacity: 1;
            }
        }

        @keyframes sway {
            0% {
                transform: translate3d(0, 0, 0);
            }

            100% {
                transform: translate3d(-20px, -10px, 0);
            }
        }

</style>

</head>

